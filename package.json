{
  "version": "1.1.0",
  "main": "dist/index.js",
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "exec": "node -r tsconfig-paths/register -r ts-node/register src/index.ts",
    "ext": "ts, js"
  },
  "scripts": {
    "dev": "nodemon .",
    "start": "node .",
    "build": "tsc && resolve-tspaths",
    "bs": "tsc && resolve-tspaths && node ."
  },
  "dependencies": {
    "@prisma/client": "^4.0.0",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "jsonwebtoken": "^8.5.1",
    "prisma": "^4.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^18.0.4",
    "nodemon": "^2.0.19",
    "resolve-tspaths": "^0.7.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.7.4"
  }
}
