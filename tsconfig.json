{
    "compilerOptions": {
        "module": "CommonJS",
        "target": "ES3",
        "esModuleInterop": true,
        "noImplicitAny": true,
        "moduleResolution": "node",
        "outDir": "dist",
        "baseUrl": ".",
        "paths": {
            "constants/*": [
                "src/constants/*"
            ]
        }
    },
    "include": [
        "src"
    ]
}