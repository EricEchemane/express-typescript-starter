{
    "compilerOptions": {
        "module": "CommonJS",
        "target": "ES3",
        "esModuleInterop": true,
        "noImplicitAny": true,
        "moduleResolution": "node",
        "sourceMap": true,
        "outDir": "dist",
        "baseUrl": ".",
        "paths": {
            "*": [
                "node_modules/*"
            ],
            "constants": [
                "src/constants/*"
            ]
        },
    },
    "include": [
        "src/**/*"
    ]
}