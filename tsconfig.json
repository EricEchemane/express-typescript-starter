{
    "compilerOptions": {
        "module": "CommonJS",
        "target": "ES6",
        "esModuleInterop": true,
        "noImplicitAny": true,
        "moduleResolution": "node",
        "outDir": "dist",
        "baseUrl": "src",
        "paths": {
            "constants/*": [
                "constants/*"
            ],
            "controllers/*": [
                "controllers/*"
            ],
            "db/*": [
                "db/*"
            ],
            "middlewares/*": [
                "middlewares/*"
            ],
            "prisma/*": [
                "prisma/*"
            ],
            "routes/*": [
                "routes/*"
            ]
        }
    },
    "include": [
        "src"
    ]
}